@using TravelTripProje.Models.Sınıflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            @foreach (var x in Model)
            {
                <h3>Hakkımızda</h3>
                <img src="@x.FotoUrl" alt="" />

                <p>@x.Aciklama </p>
            }
        </div>
    </div>

